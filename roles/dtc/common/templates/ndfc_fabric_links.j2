# This NDFC links for vPC Peering is auto-generated
# DO NOT EDIT MANUALLY
#
#jinja2: trim_blocks: True, lstrip_blocks: False
{% for link in MD_Extended.vxlan.topology.fabric_links %}
  - dst_fabric: "{{ MD.vxlan.global.name }}"
    src_interface: "{{ link.source_interface }}" 
    dst_interface: "{{ link.dest_interface }}" 
    src_device: "{{ link.source_device }}" 
    dst_device: "{{ link.dest_device }}" 
    template: "{{ link.template }}"
    profile:
        admin_state: "{{ link.admin_state }}" 
        mtu: "{{ link.mtu }}"
        peer1_description: "{{ link.source_description | default('') }}"
        peer2_description: "{{ link.destination_description | default('') }}"
{% endfor %}
